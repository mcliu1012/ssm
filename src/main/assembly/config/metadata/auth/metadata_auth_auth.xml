<?xml version="1.0" encoding="utf-8"?>
<!-- <!DOCTYPE domain SYSTEM "Metadata_Auth_Main.dtd"> -->
<domain id="admin" subsystem="fmp-admin">
	<!-- 资源分类－第一级 -->
	<resource-category key="manager" orderKey="2000" name="SSM后台管理平台" desc="SSM后台管理平台">
		<resource-category key="auth_user_manager" name="用户管理" desc="用户管理">
			<resource key="auth_user_manager_list" name="用户信息" desc="用户信息">
				<operation key="query" name="列表" desc="列表">
					<address name="用户信息列表" url="/pages/userInfo_list.shtml" />
					<address name="查询请求" url="/userInfo/query.ajax" />
					<address name="获取当前登录用户" url="/userInfo/getCurrentStaff.ajax" />
					<address name="根据名称查询用户" url="/userInfo/selectStaffByLoginName.ajax" />
				</operation>
				<operation key="detail" name="详情" desc="详情">
					<address name="用户信息详情" url="/pages/userInfo_view.shtml" />
					<address name="查询请求" url="/userInfo/get.ajax" />
				</operation>
				<operation key="add" name="新增/修改" desc="新增/修改">
					<address name="页面" url="/pages/userInfo_add.shtml" />
					<address name="新增请求" url="/userInfo/add.ajax" />
					<address name="获取详情请求" url="/userInfo/get.ajax" />
				</operation>
				<operation key="delete" name="删除" desc="删除">
					<address name="删除" url="/userInfo/delete.ajax" />
				</operation>
				<operation key="download" name="下载" desc="下载">
					<address name="下载" url="/userInfo/download.ajax" />
				</operation>
			</resource>
		</resource-category>

		<resource-category key="auth_laboratory" name="实验室" desc="实验室">
			<resource key="auth_laboratory_toolbox" name="工具箱" desc="工具箱">
				<operation key="list" name="主页面" desc="主页面">
					<address name="工具箱页面" url="/pages/toolbox.shtml" />
				</operation>
			</resource>
		</resource-category>

	</resource-category><!-- 第一级 -->
</domain>
