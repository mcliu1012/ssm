				<operation key="query" name="列表" desc="列表">
					<address name="合作伙伴列表" url="/pages/partnerInfo_list.shtml" />
					<address name="查询请求" url="/partnerInfo/query.ajax" />
				</operation>
				<operation key="detail" name="详情" desc="详情">
