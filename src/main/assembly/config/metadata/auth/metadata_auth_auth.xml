					<address name="信息展示页面" url="/pages/partnerInfo_self_view.shtml" />
					<address name="获取个人信息" url="/partnerInfo/getSelf.ajax" />
					<address name="初始化账户信息" url="/partnerInfo/initBalance.ajax" />
					<address name="初始化产品协议订单历史记录" url="/orderDealRecord/getOrderDealRecord.ajax" />
